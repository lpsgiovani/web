{
	"version": 3,
	"routes": [
		{
			"src": "^/\\.well-known(?:/.*)?$"
		},
		{
			"src": "^/((?:[^/]+/)*[^/\\.]+)$",
			"headers": {
				"Location": "/$1/"
			},
			"status": 308
		},
		{
			"src": "^/((?:[^/]+/)*[^/]+\\.\\w+)/$",
			"headers": {
				"Location": "/$1"
			},
			"status": 308
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_assets-dev/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		}
	],
	"images": {
		"sizes": [
			640,
			750,
			828,
			1080,
			1200,
			1920,
			2048,
			3840
		],
		"domains": [],
		"remotePatterns": []
	}
}